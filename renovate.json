{
  "dependencyDashboard": true,
  "groupSlug": "all",
  "lockFileMaintenance": {
    "enabled": false
  },
  "packageRules": [
    {
      "groupName": "npm",
      "groupSlug": "all",
      "matchRules": [
        {
          "matchUpdateTypes": ["minor", "patch"],
          "matchDatasources": ["npm"],
          "matchPackagePatterns": ["package.json"],
          "dependencyDashboardApproval": true
        }
      ]
    },
    {
      "groupName": "docker",
      "groupSlug": "all",
      "matchRules": [
        {
          "matchDatasources": ["docker"]
        }
      ]
    }
  ]
}
